<% for(var i=0; i<users.length; i++) { %>
<tr>
    <!-- d dapat sudlan ug space sa code na pareha naa sa baba (kanang naay include) kay mag error tungod sa whitespace -->
    <td><%=i+1%></td>
    <td><%=users[i].name%></td>
    <td><%=users[i].email%></td>
    <td><%=users[i].gender%></td>
    <td><%=users[i].status%></td>
    <td>
        <a href="/update-user?id=<%=users[i]._id%>" but class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow deleteButton" data-id=<%=users[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>